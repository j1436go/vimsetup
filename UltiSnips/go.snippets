	${3}
}
endsnippet

snippet errc "Checks and logs error"
if err != nil {
	log.Println(err)
}
${1}
endsnippet

snippet hhandler "An http handler"
func ${1}(w http.ResponseWriter, r *http.Request) {
	w.Write(${2})
}
endsnippet
